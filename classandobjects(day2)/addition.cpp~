#include<iostream>
#include<string.h>
using namespace std;
class Add
{
 public:
  void add(int a,int b)
  {
  int res=a+b;
  cout<<"ADDITION OF INTEGER NUMBER IS::"<<res<<endl;
  }

  void add(float a,float b)
  {
  float res=a+b;
  cout<<"ADDITION OF FLOATING NUMBER IS::"<<res<<endl;
  }
};

int main()
{
Add a;
a.add(5,6);
a.add(5.6f,2.3f);
}
