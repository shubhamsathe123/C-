#include<iostream>
#include<conio.h>
using name space std;
class base
{
    public:
    int i;
};
class derived1: virtual public base
{
    public:
    int j;
};
class derived 2:virtual public base
{
    public:
    int k;
};
class derived 3: public derived1,public derived2
{
    public:
    int sum;
};
void main()
{
    derived 3 ob;
    ob.i=10;
    ob.j=20;
    ob.k=30;
    ob.sum=ob.i+ob.j+ob.k;
    cout<<ob.i<<"";
    cout<<ob.j<<""<<ob.k<<"";
    cout<<ob.sum;
    getch();
}

